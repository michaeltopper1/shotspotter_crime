	institution = {Social Science Research Network},
	author = {Raghav, Manu and Diette, Timothy M.},
	month = aug,
	year = {2021},
	doi = {10.2139/ssrn.3908875},
	keywords = {Greek Myth or Fact? The Role of Greek Houses in Alcohol and Drug Violations on American Campuses, Manu Raghav, SSRN, Timothy M. Diette},
