@article{schmidheiny_event_2020,
	title = {On {Event} {Studies} and {Distributed}-{Lags} in {Two}-{Way} {Fixed} {Effects} {Models}: {Identification}, {Equivalence}, and {Generalization}},
	issn = {1556-5068},
	shorttitle = {On {Event} {Studies} and {Distributed}-{Lags} in {Two}-{Way} {Fixed} {Effects} {Models}},
	url = {https://www.ssrn.com/abstract=3571164},
	doi = {10.2139/ssrn.3571164},
	abstract = {We discuss properties and pitfalls of panel-data event study designs. We derive three main results. First, assuming constant treatment eﬀects before and/or after some event time, also known as binning, is a natural restriction imposed on theoretically inﬁnite eﬀect windows. Binning identiﬁes dynamic treatment eﬀects in the absence of nevertreated units and is particularly suitable in case of multiple events. Second, event study designs with binned endpoints and distributed-lag models are numerically identical leading to the same parameter estimates after correct reparametrization. Third, classic dummy variable event study designs can be generalized to models that account for multiple events of diﬀerent sign and intensity of the treatment, which are common in public and labor economics. We demonstrate the practical relevance of our methodological points in an application studying the eﬀects of unemployment beneﬁt duration on job search eﬀort.},
	language = {en},
	urldate = {2021-05-28},
	journal = {SSRN Journal},
	author = {Schmidheiny, Kurt and Siegloch, Sebastian},
	year = {2020},
	file = {Schmidheiny and Siegloch - 2020 - On Event Studies and Distributed-Lags in Two-Way F.pdf:/Users/michaeltopper/Zotero/storage/H7GC7H49/Schmidheiny and Siegloch - 2020 - On Event Studies and Distributed-Lags in Two-Way F.pdf:application/pdf},
}

