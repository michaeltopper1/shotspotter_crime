@article{chaln_pro-social_nodate,
	title = {The {Pro}-{Social} {Motivations} of {Police} {Oﬃcers}},
	abstract = {How do public sector workers balance their pro-social motivations with private interests? In this study of police oﬃcers, we exploit two institutional features that change the implicit cost of making an arrest: arrests made near the end of an oﬃcer’s shift are more likely to require overtime work, and arrests made on days when an oﬃcer “moonlights” at an oﬀ-duty job after their shift have a higher opportunity cost. We document two consequences for oﬃcer behavior. First, contrary to popular wisdom, oﬃcers reduce arrests near the end of their shift, and the quality of arrests increases. We argue that these patterns are driven by oﬃcer preferences rather than departmental policy, fatigue, or incapacitation from earlier arrests. Second, oﬃcers further reduce late-shift arrests on days in which they moonlight after work, suggesting that they are, in fact, modestly responsive to ﬁnancial incentives. Using these results, we estimate a dynamic model that identiﬁes an oﬃcer’s implied trade-oﬀ between private and pro-social motivations. We ﬁnd that police oﬃcers exhibit high pro-social motivation towards their work. In contrast to prior research showing that law enforcement outcomes are sensitive to ﬁnancial incentives at an institutional level, the behavior of individual oﬃcers — the “street-level bureaucrats” who enforce the law — is not meaningfully distorted by monetary considerations.},
	language = {en},
	author = {Chalﬁn, Aaron and Goncalves, Felipe},
	file = {Chalﬁn and Goncalves - The Pro-Social Motivations of Police Oﬃcers.pdf:/Users/michaeltopper/Zotero/storage/8DRJ7ER5/Chalﬁn and Goncalves - The Pro-Social Motivations of Police Oﬃcers.pdf:application/pdf},
}

