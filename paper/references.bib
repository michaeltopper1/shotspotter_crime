@article{doucette_impact_2021,
	title = {Impact of {ShotSpotter} {Technology} on {Firearm} {Homicides} and {Arrests} {Among} {Large} {Metropolitan} {Counties}: a {Longitudinal} {Analysis}, 1999–2016},
